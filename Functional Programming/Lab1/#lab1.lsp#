( list список-студентов
("Иванов" ("Информатика" 5), ("Физика" 4), ("Алгебра" 3))
("Петров" ("Геометрия" 3), ("География" 4), ("Биология" 5))
("Сидоров" ("Математика" 3), ("Физкультура" 5), ("ОБЖ" 5))
)

(defun avg-grade (student-grades)
  (apply '+ student-grades)
  (/ (apply '+ student-grades) (length student-grades)))

(mapcar #'avg-grade список-студентов)



